{
  "name": "iab-adcom",
  "version": "0.2.8",
  "description": "TypeScript definitions for IAB Tech Lab's AdCOM (Advertising Common Object Model)",
  "scripts": {
    "build": "tsup && tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "AdCOM",
    "IAB Tech Lab",
    "OpenRTB",
    "typescript",
    "types",
    "definitions",
    "advertising",
    "programmatic"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./media": {
      "import": "./src/media/index.ts",
      "require": "./src/media/index.ts"
    },
    "./context": {
      "import": "./src/context/index.ts",
      "require": "./src/context/index.ts"
    },
    "./placement": {
      "import": "./src/placement/index.ts",
      "require": "./src/placement/index.ts"
    },
    "./enum": {
      "import": "./src/enum/index.ts",
      "require": "./src/enum/index.ts"
    }
  },
  "author": {
    "name": "Kai Miyamoto",
    "url": "https://github.com/hogekai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hogekai/iab-types.git",
    "directory": "packages/types-iab-adcom"
  },
  "bugs": {
    "url": "https://github.com/hogekai/iab-types.git"
  },
  "homepage": "https://github.com/hogekai/iab-types.git",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./media": {
        "import": "./dist/media/index.d.ts",
        "require": "./dist/media/index.d.ts",
        "types": "./dist/media/index.d.ts"
      },
      "./context": {
        "import": "./dist/context/index.d.ts",
        "require": "./dist/context/index.d.ts",
        "types": "./dist/context/index.d.ts"
      },
      "./placement": {
        "import": "./dist/placement/index.d.ts",
        "require": "./dist/placement/index.d.ts",
        "types": "./dist/placement/index.d.ts"
      },
      "./enum": {
        "import": "./dist/enum/index.mjs",
        "require": "./dist/enum/index.js",
        "types": "./dist/enum/index.d.ts"
      }
    }
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
